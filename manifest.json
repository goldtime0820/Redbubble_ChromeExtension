{
	"manifest_version": 2,
	"name": "Redbubble",
	"version": "1.0",
	"icons": {
		"128": "icon.png"	},
	"content_scripts": [
		{
			"matches": ["https://www.redbubble.com/portfolio/images/new*"],
			"js": ["./js/jquery.min.js","./js/content.js","./js/float-panel.js"],
			"css": ["./css/styles.css"]
		}
	],
	"browser_action": {
		"default_title": "Rebubble",
		"default_popup": "popup.html"
	},
	"permissions": [
		"http://*/*",
		"https://*/*"
	]
}